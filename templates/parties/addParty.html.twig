{% extends 'base.html.twig' %}
{%block title %}
    <h1>{{ title }}</h1>
{% endblock %}

{% block body %}
    {{ form_start(form) }}
        <div>
            {{ form_row(form.name) }}
        </div>
    <div>
        {{ form_row(form.description) }}
    </div>
    <div>
        {{ form_row(form.date) }}
    </div>
    <div>
        {{ form_row(form.image) }}
    </div>
    <div>
        {{ form_row(form.submit) }}
    </div>
    {{ form_end(form) }}
{% endblock%}


